  0% { transform: scale(0.95); }
  66% { transform: scale(1.05); }
  100% { transform: scale(1); }
}

@keyframes fade-in { 
  0% { opacity: 0; }
  95%, 100% { opacity: 1; }
}

@keyframes fade-out { 
  0% { opacity: 1; }
  95%, 100% { opacity: 0; }
}

@keyframes spin { 
  0% { transform: rotate(0deg); }
  100% { transform: rotate(1turn); }
}

@keyframes spin-dash { 
  0% { stroke-dasharray: 1, 200; stroke-dashoffset: 0; }
  50% { stroke-dasharray: 90, 150; stroke-dashoffset: -40px; }
  100% { stroke-dasharray: 90, 150; stroke-dashoffset: -120px; }
}

@keyframes spin-scaleY { 
  0%, 40%, 100% { transform: scaleY(0.4); }
  20% { transform: scaleY(1); }
}

@keyframes jump-scaleY { 
  0% { transform: translateY(0px) scaleY(1); }
  7.5% { transform: translateY(-120%) scaleY(2); }
  15%, 35% { transform: translateY(-120%) scaleY(0.5); }
  43.5% { transform: translateY(0px) scaleY(2); }
  52.5%, 72.5% { transform: translateY(120%) scaleY(0.5); }
  80%, 100% { transform: translateY(0px) scaleY(1); }
}

